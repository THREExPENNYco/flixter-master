<br /> 
<div class="booyah-box col-qo offset-1 text-center"> 
  <h1>Add a new course</h1> 
  <%= simple_form_for @course, url: instructor_courses_path do |f| %>  
      <%= f.input :title %> 
      <%= f.input :description %> 
      <%= f.input :cost %> 
      <%= f.file_field :image %> 
      <br /> 
      <%= f.submit "Create", class: 'btn btn-primary' %> 
      <br /> 
      <% end %> 
      <br /> 
</div> 